{
   "name": "data-dashboard",
   "description": "A data-driven dashboard console for report widgets",
   "version": "0.1.1",
   "license": "MIT",
   "main": "server.js",
   "type": "module",
   "browser": "docs/index.html",
   "repository": "github:dnajs/data-dashboard",
   "homepage": "https://data-dashboard.js.org",
   "bugs": "https://github.com/dnajs/data-dashboard/issues",
   "keywords": [
      "charts",
      "dashboard",
      "dnajs",
      "html5",
      "javascript",
      "report",
      "widgets"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "browser": true,
      "jquery": true,
      "mocha": true,
      "node": true,
      "globals": {
         "app": true,
         "dna": false,
         "fetchJson": false,
         "library": false
      }
   },
   "scripts": {
      "pretest": "jshint *.js src spec && rimraf build **/.DS_Store && gulp build",
      "test": "mocha spec",
      "posttest": "gulp minify && gulp hash && rimraf docs && gulp docs",
      "dev": "npm test && gulp watch",
      "prod": "npm test && echo && echo 'Visit --> http://localhost:7531' && npm start"
   },
   "cdnVersion": {
      "fontAwesome": "5.15"
   },
   "dependencies": {
      "chart.js": "3.0.0-beta.7",
      "dna.js": "~1.7",
      "fetch-json": "~2.4",
      "jquery": "~3.5",
      "moment": "~2.29",
      "node-fetch": "~2.6",
      "pretty-print-json": "~0.4",
      "select2": "~4.0",
      "simple-datatables": "~2.1",
      "web-ignition": "~1.3",
      "whatwg-fetch": "~3.5"
   },
   "devDependencies": {
      "@babel/core": "~7.12",
      "@babel/preset-env": "~7.12",
      "@fortawesome/fontawesome-free": "~5.15",
      "babel-minify": "~0.5",
      "browser-sync": "~2.26",
      "canvas": "~2.6",
      "cssnano": "~4.1",
      "express": "~4.17",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-concat": "~2.6",
      "gulp-file-include": "~2.3",
      "gulp-header": "~2.0",
      "gulp-htmlhint": "~4.0",
      "gulp-less": "~4.0",
      "gulp-order": "~1.2",
      "gulp-postcss": "~9.0",
      "gulp-replace": "~1.0",
      "gulp-rev-all": "~2.0",
      "gulp-size": "~3.0",
      "gulp-touch-cmd": "~0.0",
      "gulp-w3c-html-validator": "~2.0",
      "jsdom": "~16.4",
      "jshint": "~2.12",
      "merge-stream": "~2.0",
      "mocha": "~8.2",
      "postcss": "~8.2",
      "postcss-font-magician": "~3.0",
      "postcss-preset-env": "~6.7",
      "server-listening": "~0.2"
   }
}
